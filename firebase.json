{
  "database": {
    "rules": "config/database.rules.json"
  },
  "storage": {
    "rules": "config/storage.rules"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "config/*",
      "reports/*",
      "firebase.json"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
